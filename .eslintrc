{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
  ],
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "$": true,
    "NOHMR": true,
    "HOSTNAME": true,
    "CSSMODULES": true,
    "ASYNC_ROUTES": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": [2, { "allow": ["error", "info"] }],
    "strict": [0, "global"],
    "react/require-default-props": "off",
    "no-underscore-dangle": "off",
    "global-require": "off",
    "react/sort-comp": "off",
    "react/no-array-index-key": "off",
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off"
  },
  "settings": {
  }
}
